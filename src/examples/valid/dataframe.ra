func main(): void {
  dataframe = read_csv("song_data_clean.csv");
  avg = average(dataframe, "song_duration_ms");
  col = "danceability";
  danceability_std = std(dataframe, col);
  danceability_mode = median(dataframe, col);
  danceability_var = variance(dataframe, col);
  corr = correlation(dataframe, col, "song_duration_ms");
  print(avg, danceability_std, danceability_mode, danceability_var, corr);
}
